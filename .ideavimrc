" vim.g.mapleader = '\\'
"
" local keymap = vim.keymap
" keymap.set('n', '<leader>nh', ':nohl<cr>')
" keymap.set('n', 'vh', ':vsplit<cr>')
" keymap.set('n', 'vs', ':split<cr>')
let mapleader = " "
nmap <leader>- <C-x>
nmap <leader>= <C-a>

nnoremap vh :vsplit<cr>
nnoremap vs :split<cr>
nmap fs :w<cr>
nmap <C-l> <C-w>l
nmap <C-k> <C-w>k
nmap <C-h> <C-w>h
nmap <C-j> <C-w>j
nmap tn :tabnext<cr>
nmap tp :tabp<cr>
nmap vx :close<cr>
nmap va :qa<cr>
nmap mu :m-2<cr>
nmap md :m+<cr>

unmap <C-k>

set clipboard+=unnamedplus
